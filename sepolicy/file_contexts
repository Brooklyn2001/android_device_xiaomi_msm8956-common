# Cache
/dev/block/platform/soc.0/7824900.sdhci/by-name/cache                 u:object_r:cache_block_device:s0

# DSP
/dev/block/platform/soc.0/7824900.sdhci/by-name/dsp                   u:object_r:dsp_block_device:s0

# IR Blaster
/dev/lirc[0-9]*                                                       u:object_r:lirc_device:s0

# Fingerprint
/data/fpc(/.*)?                                         u:object_r:fpc_data_file:s0
/data/fpc/s(/.*)?                                       u:object_r:fpc_stat_data_file:s0
/dev/socket/fpce                                        u:object_r:fpc_socket:s0
/sys/devices/soc.0/fpc_fpc1020\.[0-9]+/irq              u:object_r:fpc_sysfs:s0
/sys/devices/soc.0/fpc_fpc1020\.[0-9]+/enable_wakeup    u:object_r:fpc_sysfs:s0

# Decrypt
/data/decrypt.txt                                       u:object_r:decrypt_file:s0

# mmbn-daemon
/system/bin/mmbn-daemon                                 u:object_r:mmbn-daemon_exec:s0

# Kcal
/sys/devices/platform/kcal_ctrl.0/kcal_hue              u:object_r:fuckse_sysfs:s0
/sys/devices/platform/kcal_ctrl.0/kcal_sat              u:object_r:fuckse_sysfs:s0
/sys/devices/platform/kcal_ctrl.0/kcal_val              u:object_r:fuckse_sysfs:s0
/sys/devices/platform/kcal_ctrl.0/kcal_cont             u:object_r:fuckse_sysfs:s0

# Sound control
/sys/kernel/sound_control/headphone_gain                u:object_r:fuckse_sysfs:s0
/sys/kernel/sound_control/speaker_gain                  u:object_r:fuckse_sysfs:s0
/sys/kernel/sound_control/mic_gain                      u:object_r:fuckse_sysfs:s0
